<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andres.proyectofinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PrincipalActivity" />
        <activity android:name=".UserListActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".CalculadoraActivity" />
        <activity
            android:name=".MemoryActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".MusicaActivity" />
        <activity android:name=".MemoryRankingActivity" />
        <activity android:name=".PagerHolder" />
        <activity android:name=".PerfilActivity" />
        <activity android:name=".CambiarContrasenaActivity"></activity>
    </application>

</manifest>
